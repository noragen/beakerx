  "compilerOptions": {
    "declaration": true,
    "declarationDir": "../dist",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "lib": ["dom", "es5", "es2015"],
